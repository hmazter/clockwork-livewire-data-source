{
  "name": "hmazter/clockwork-livewire-data-source",
  "description": "Laravel Livewire data source for Clockwork",
  "type": "library",
  "license": "MIT",
  "version": "0.1.1",
  "authors": [
    {
      "name": "Kristoffer HÃ¶gberg"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "itsgoingd/clockwork": "^5.3",
    "livewire/livewire": "^3.6"
  },
  "require-dev": {
    "laravel/pint": "^1.25",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "Hmazter\\ClockworkLivewireDataSource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "@php ./vendor/bin/phpunit",
    "lint": "@php ./vendor/bin/pint"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hmazter\\ClockworkLivewireDataSource\\ClockworkLivewireServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
